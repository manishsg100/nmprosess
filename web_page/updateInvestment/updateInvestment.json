        "\t\t\tdescription: $data.investmentObj.description !== undefined ? $data.investmentObj.description : null,",
        "\t\t\thighlights: $data.investmentObj.highlights !== undefined ? $data.investmentObj.highlights : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
