    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.resolutionObject ){",
        "\treturn {",
        "\t\t//map resolutionObject variable to expected task contract input",
        "\t\tresolutionObjectInput: {",
        "\t\t\tid: $data.resolutionObject.id !== undefined ? $data.resolutionObject.id : null,",
        "\t\t\tstream: $data.resolutionObject.stream !== undefined ? $data.resolutionObject.stream : null,",
        "\t\t\tresolution: $data.resolutionObject.resolution !== undefined ? $data.resolutionObject.resolution : null,",
        "\t\t\tresDate: $data.resolutionObject.resDate !== undefined ? $data.resolutionObject.resDate : null,",
        "\t\t\tapprover: $data.resolutionObject.approver !== undefined ? $data.resolutionObject.approver : null,",
        "\t\t\tisApprovalNeeded: $data.resolutionObject.isApprovalNeeded !== undefined ? $data.resolutionObject.isApprovalNeeded : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
